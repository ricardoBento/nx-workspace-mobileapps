        <icon height="76" platform="ios" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="29" platform="ios" src="res/icon/ios/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="res/icon/ios/icon-small@2x.png" width="58" />
        <icon height="87" platform="ios" src="res/icon/ios/icon-small@3x.png" width="87" />
        <icon height="1024" platform="ios" src="res/icon/ios/icon-1024.png" width="1024" />
        <splash height="1136" platform="ios" src="res/screen/ios/Default-Portrait-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="res/screen/ios/Default-Portrait-667h.png" width="750" />
        <splash height="2208" platform="ios" src="res/screen/ios/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="res/screen/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="res/screen/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="768" platform="ios" src="res/screen/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="res/screen/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="res/screen/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="960" platform="ios" src="res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="res/screen/ios/Default~iphone.png" width="320" />
        <splash height="320" platform="ios" src="res/screen/ios/Default-Landscape~iphone.png" width="480" />
        <splash height="640" platform="ios" src="res/screen/ios/Default-Landscape@2x~iphone.png" width="960" />
        <splash height="640" platform="ios" src="res/screen/ios/Default-Landscape-568h@2x~iphone.png" width="1136" />
        <splash height="750" platform="ios" src="res/screen/ios/Default-Landscape-667h.png" width="1334" />
        <splash height="2732" platform="ios" src="res/screen/ios/Default@2x~ipad~anyany.png" width="2732" />
    </platform>
    <access origin="https://mm-upgrade-staging.flowhospitalitytraining.co.uk" />
    <access origin="*://flow-transition-staging.s3.amazonaws.com" />
    <access origin="*://*.flowmodulemanager.co.uk" />
    <access origin="*://*.s3.amazonaws.com" />
    <access origin="*://*.google.com" />
    <access origin="*://*.google.co.uk" />
    <access origin="*://platform.twitter.com" />
    <access origin="*://*.googleapis.com" />
    <access origin="*://*.googleapis.co.uk" />
    <allow-navigation href="*" />
    <allow-navigation href="*://*.s3.amazonaws.com/*" />
    <allow-navigation href="*://*.flowhospitalitytraining.co.uk/*" />
    <allow-navigation href="*://*.flowmodulemanager.co.uk/*" />
    <allow-navigation href="*://*.youtube.com/*" />
    <access origin="*://*.youtube.com" />
    <access origin="*://archive.org" />
    <access origin="*://*.googlevideo.com" />
    <access origin="*://*.ytimg.com" />
    <access origin="*://*.youtube.com" />
    <access origin="*://*.youtube-nocookie.com" />
    <access origin="*://dailymotion.com" />
    <allow-navigation href="*://*.vimeo.com/*" />
    <access origin="*://*.vimeo.com" />
    <access origin="*://*.vimeocdn.com" />
    <access origin="*://*.akamaized.net" />
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
    </feature>
    <feature name="FileTransfer">
        <param name="ios-package" value="CDVFileTransfer" />
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <preference name="Scheme" value="http" />
    <preference name="iosScheme" value="http" />
    <preference name="MixedContentMode" value="0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <plugin name="cordova-plugin-ionic-webview" spec="^4.0.0" />
    <plugin name="cordova-plugin-ionic-keyboard" spec="^2.0.5" />
    <plugin name="cordova-plugin-console" spec="^1.1.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.3" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.2" />
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="cordova-plugin-file-opener2" spec="^3.0.5">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-globalization" spec="^1.11.0" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.2" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.2" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.4" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="cordova-plugin-vibration" spec="^3.1.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.4" />
    <plugin name="phonegap-plugin-push" spec="^2.3.0">
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
        <variable name="FCM_VERSION" value="17.0.+" />
    </plugin>
