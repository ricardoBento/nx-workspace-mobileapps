<?xml version='1.0' encoding='utf-8'?>
<widget id="uk.co.flowhospitalitytraining.flowzone" version="2.5.73" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ionic cli</name>
    <description>
        Flow delivers a continuously evolving learning and development solution.
    </description>
    <author email="support@flowhospitalitytraining.co.uk" href="http://www.flowhospitalitytraining.co.uk">
        Flow Hospitality Training Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="EnableViewportScale" value="true" />
    <preference name="target-device" value="handset" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="29" />
    <preference name="android-windowSoftInputMode" value="adjustResize|stateHidden" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="500" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
    <platform name="android">
        <allow-intent href="market:*" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <icon density="ldpi" src="res/icon/android/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="res/icon/android/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="res/icon/android/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="res/icon/android/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="res/icon/android/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="res/icon/android/drawable-xxxhdpi-icon.png" />
        <splash density="port-ldpi" src="res/screen/android/drawable-ldpi/splash.png" />
        <splash density="port-mdpi" src="res/screen/android/drawable-mdpi/splash.png" />
        <splash density="port-hdpi" src="res/screen/android/drawable-hdpi/splash.png" />
        <splash density="port-xhdpi" src="res/screen/android/drawable-xhdpi/splash.png" />
        <splash density="port-xxhdpi" src="res/screen/android/drawable-xxhdpi/splash.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/drawable-xxxhdpi/splash.png" />
        <resource-file src="res/icon/android/push/drawable/push_notification_logo.png" target="app/src/main/res/drawable/push_notification_logo.png" />
        <resource-file src="res/icon/android/push/drawable-mdpi/push_notification_logo.png" target="app/src/main/res/drawable-mdpi/push_notification_logo.png" />
        <resource-file src="res/icon/android/push/drawable-hdpi/push_notification_logo.png" target="app/src/main/res/drawable-hdpi/push_notification_logo.png" />
        <resource-file src="res/icon/android/push/drawable-xhdpi/push_notification_logo.png" target="app/src/main/res/drawable-xhdpi/push_notification_logo.png" />
        <resource-file src="res/icon/android/push/drawable-xxhdpi/push_notification_logo.png" target="app/src/main/res/drawable-xxhdpi/push_notification_logo.png" />
        <resource-file src="res/icon/android/push/drawable-xxxhdpi/push_notification_logo.png" target="app/src/main/res/drawable-xxxhdpi/push_notification_logo.png" />
    </platform>
    <config-file parent="/manifest/application" target="AndroidManifest.xml">
        <meta-data android:name="com.google.firebase.messaging.default_notification_icon" android:resource="@drawable/push_notification_logo" />
    </config-file>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="WKWebViewOnly" value="true" />
        <plugin name="InAppBrowser" value="CDVInAppBrowser" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="UIWebViewBounce" value="false" />
        <resource-file src="GoogleService-Info.plist" />
        <feature name="CDVWKWebViewEngine">
            <param name="ios-package" value="CDVWKWebViewEngine" />
        </feature>
        <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
        <icon height="57" platform="ios" src="res/icon/ios/icon.png" width="57" />
        <icon height="114" platform="ios" src="res/icon/ios/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="res/icon/ios/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="res/icon/ios/icon-40@2x.png" width="80" />
